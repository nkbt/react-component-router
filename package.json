{
  "name": "react-component-router",
  "version": "2.0.0-preview",
  "description": "Routing solution for React loosely based on ComponentRouter",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "config": {
    "component": "ReactComponentRouter",
    "externals": {
      "react": "React",
      "react-dom": "ReactDOM",
      "history": "History",
      "qs": "Qs"
    },
    "include": [
      "https://unpkg.com/react@16.0.0/umd/react.production.min.js",
      "https://unpkg.com/react-dom@16.0.0/umd/react-dom.production.min.js",
      "https://unpkg.com/redux/dist/redux.min.js",
      "https://unpkg.com/qs/dist/qs.js",
      "https://unpkg.com/history/umd/history.min.js",
      "https://unpkg.com/component-router/build/component-router.min.js",
      "https://unpkg.com/react-component-router/build/react-component-router.min.js"
    ]
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-cli serve",
    "build": "NODE_ENV=production webpack-cli",
    "lint": "eslint . --fix",
    "check:lint": "eslint .",
    "test": "./scripts/test.js src/**/*.test.js",
    "cov": "nyc --reporter=text ./scripts/test.js src/**/*.test.js",
    "check:cov": "nyc --reporter=html --check-coverage ./scripts/test.js src/**/*.test.js | tap-junit --output ./reports/test",
    "e2e": "exit 0",
    "check:e2e": "exit 0",
    "deps": "depcheck",
    "check:deps": "[ ! $(depcheck | grep --invert-match 'No depcheck issue') ]",
    "pretty": "prettier ./src --write",
    "check:pretty": "prettier ./src --check",
    "csslint": "stylelint --color 'example/**/*.css' --fix",
    "check:csslint": "stylelint --color 'example/**/*.css'",
    "ci": "circleci local execute"
  },
  "peerDependencies": {
    "history": "^5",
    "prop-types": "^15",
    "qs": "^6",
    "react": "^17"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.13",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "depcheck": "^1.3.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "history": "^5.0.0",
    "html-webpack-plugin": "next",
    "mini-css-extract-plugin": "^1.3.3",
    "nyc": "^15.1.0",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.2",
    "tap-junit": "^4.2.0",
    "tape": "^5.1.1",
    "webpack": "^5.11.1",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^3.0.0"
  }
}
