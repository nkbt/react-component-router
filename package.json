{
  "name": "react-component-router",
  "version": "2.0.0-preview",
  "description": "Routing solution for React loosely based on ComponentRouter",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "config": {
    "component": "ReactComponentRouter",
    "externals": {
      "react": "React",
      "react-dom": "ReactDOM",
      "history": "History"
    },
    "include": [
      "https://unpkg.com/react@16.0.0/umd/react.production.min.js",
      "https://unpkg.com/react-dom@16.0.0/umd/react-dom.production.min.js",
      "https://unpkg.com/redux/dist/redux.min.js",
      "https://unpkg.com/history/umd/history.min.js",
      "https://unpkg.com/component-router/build/component-router.min.js",
      "https://unpkg.com/react-component-router/build/react-component-router.min.js"
    ]
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-cli serve",
    "build": "NODE_ENV=production webpack-cli",
    "lint": "eslint . --fix",
    "check:lint": "eslint .",
    "deps": "depcheck",
    "check:deps": "[ ! $(depcheck | grep --invert-match 'No depcheck issue') ]",
    "pretty": "prettier ./src --write",
    "check:pretty": "prettier ./src --check",
    "csslint": "stylelint --color 'example/**/*.css' --fix",
    "check:csslint": "stylelint --color 'example/**/*.css'",
    "ci": "circleci local execute",
    "deploy": "gh-pages --dist dist --branch gh-pages --no-history --user 'Nikita Butenko <nik@butenko.me>'"
  },
  "peerDependencies": {
    "history": "^5",
    "prop-types": "^15",
    "react": "^17"
  },
  "dependencies": {
    "@nkbt/shallow-equal-objects": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "css-loader": "^6.5.1",
    "depcheck": "^1.4.2",
    "eslint": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^3.2.3",
    "history": "^5.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.3",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-prettier": "^2.0.0",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-manifest-plugin": "^4.0.2"
  }
}
